CREATE OR REPLACE PROCEDURE PR_UPDATE_COUNTRIES 
(
 PRM_COUNT_ID IN VARCHAR2
,PRM_COUNT_NAME IN VARCHAR2
,PRM_REGION_ID IN NUMBER
)
AS 
BEGIN
  UPDATE countries
SET
  country_id =  PRM_COUNT_ID
, country_name = PRM_COUNT_NAME
, region_id = PRM_REGION_ID
WHERE
    country_id = PRM_COUNT_ID;
END PR_UPDATE_COUNTRIES;