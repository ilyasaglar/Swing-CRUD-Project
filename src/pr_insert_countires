CREATE OR REPLACE PROCEDURE PR_INSERT_COUNTRIES 
(
 PRM_COUNT_ID IN VARCHAR2
,PRM_COUNT_NAME IN VARCHAR2
,PRM_REGION_ID IN NUMBER
)
AS 
BEGIN
  INSERT INTO countries (
    country_id,
    country_name,
    region_id
) VALUES (
    PRM_COUNT_ID,
    PRM_COUNT_NAME,
    TO_CHAR(PRM_REGION_ID)
);
END PR_INSERT_COUNTRIES;