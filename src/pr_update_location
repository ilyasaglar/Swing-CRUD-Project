CREATE OR REPLACE PROCEDURE PR_UPDATE_LOCATION 
(
 PRM_LOC_ID IN NUMBER
,PRM_STR_ADD IN VARCHAR2
,PRM_POST_CODE IN VARCHAR2
,PRM_CITY IN VARCHAR2
,PRM_STATE IN VARCHAR2
,PRM_COUNTRY_ID IN VARCHAR2
)
AS 
BEGIN
 UPDATE locations
SET
    street_address =PRM_STR_ADD,
    postal_code =PRM_POST_CODE,
    city =PRM_CITY,
    state_province =PRM_STATE,
    country_id =to_char(PRM_COUNTRY_ID)
WHERE
    location_id = prm_loc_id;
END;